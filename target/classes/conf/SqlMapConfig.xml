	"dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="work.user.UserBean" alias="user"/>
		<typeAlias type="work.course.CourseBean" alias="course"/>
		<typeAlias type="work.professorreg.ProfessorregBean" alias="professorreg"/>
		<typeAlias type="work.studentreg.StudentregBean" alias="studentreg"/>
